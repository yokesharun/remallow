{
  "name": "remallow",
  "version": "1.0.8",
  "description": "GUI for NPM Package Managing!",
  "scripts": {
    "server-build": "webpack --mode=production --config ./webpack.server.config.js",
    "ui-build": "webpack --mode=production --config ./webpack.ui.config.js",
    "ui": "webpack-dev-server --mode=development --config ./webpack.ui.config.js",
    "server": "nodemon ./server/bin/index.js localhost 8081",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run ui-build && npm run server-build"
  },
  "main": "./server/bin/index.js",
  "bin": {
    "remallow": "./dist/server/main.js"
  },
  "keywords": [
    "react",
    "npm",
    "yarn",
    "package",
    "package manager",
    "npm gui",
    "gui",
    "react gui",
    "remallow",
    "yarn package manager",
    "npm package manager"
  ],
  "bugs": {
    "url": "https://github.com/yokesharun/remallow/issues",
    "email": "ayokesh.cse@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yokesharun/remallow.git"
  },
  "author": "Arun Yokesh",
  "license": "MIT",
  "homepage": "https://github.com/yokesharun/remallow#readme",
  "dependencies": {
    "chrome-launcher": "^0.15.0",
    "colors": "^1.4.0",
    "express": "^4.17.1",
    "minimist": "^1.2.5",
    "react-github-btn": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "axios": "^0.24.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.21",
    "node-loader": "^2.0.0",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-node-externals": "^3.0.0"
  },
